{
  "name": "dox-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start:dev": "set NODE_ENV=dev&&nest start --watch",
    "start:prod": "export NODE_ENV=prod && node dist/main",
    "fmt": "eslint --cache **/*.ts --fix",
    "test": "jest --detectOpenHandles --verbose",
    "test:watch": "test jest --watch",
    "test:cov": "test jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test&&jest --config ./test/jest-e2e.json",
    "pretypeorm": "ts-node -r tsconfig-paths/register src/database/fixconfig.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ormconfig.json",
    "migration:generate": "set NODE_ENV=dev&&yarn typeorm migration:generate -n NEW",
    "migration_dev": "set NODE_ENV=dev&&yarn typeorm migration:run",
    "migration_prod": "export NODE_ENV=prod && yarn typeorm migration:run"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.5",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.2.5",
    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/typeorm": "^8.0.2",
    "@network-utils/tcp-ping": "^1.2.1",
    "argon2": "^0.28.3",
    "class-validator": "^0.13.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "follow-redirects": "^1.14.7",
    "hash-sum": "^2.0.0",
    "multer": "^1.4.4",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "shelljs": "^0.8.5",
    "typeorm": "^0.2.41",
    "uuid-v4-validator": "^2.0.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.5",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.4",
    "@types/express": "^4.17.13",
    "@types/hash-sum": "^1.0.0",
    "@types/jest": "27.4.0",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.5",
    "@types/prettier": "^2.4.2",
    "@types/sinon": "^10.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "jest": "^27.4.5",
    "prettier": "^2.4.1",
    "sinon": "^12.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
